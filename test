# !/bin/bash

printf "Here's some text to test gitHelper staging." > ./testData/sample
printf "Here's some more text to test gitHelper staging." > ./testData/data/sample\ data
printf "Here's some more text in a new untracked file to test gitHelper staging." > ./testData/untracked\ file



./gitHelper



printf "" > ./testData/sample
printf "" > ./testData/data/sample\ data
rm ./testData/untracked\ file